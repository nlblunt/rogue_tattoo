<%# add navigation links to this file %>
<li class="navbar-center-item"><%= link_to 'Rogue Tattoo', root_path, class: 'navbar-text-color' %></li>
<li class="dropdown navbar-center-item">
        <a href="#" class="dropdown-toggle navbar-text-color" data-toggle="dropdown">Artists <b class="caret"></b></a>
        <ul class="dropdown-menu drop-menu-item navbar-color">
        	<% @art = Artist.all %>
        	<% @art.each do |artist| %>
          	<li class="navbar-text-color drop-menu-item"><%= link_to artist.name, artist_path(artist) %></li>
        	<% end %>
        </ul>
</li>
<li class="dropdown navbar-center-item">
        <a href="#" class="dropdown-toggle navbar-text-color" data-toggle="dropdown">Cosmetic Tattoos<b class="caret"></b></a>
        <ul class="dropdown-menu drop-menu-item navbar-color">
        	<% @makeupart = Makeupartist.all %>
        	<% @makeupart.each do |m_artist| %>
          	<li class="navbar-text-color drop-menu-item"><%= link_to m_artist.name, makeupartist_path(m_artist) %></li>
        	<% end %>
        </ul>
</li>
<li class="navbar-center-item"><%= link_to "About Us", about_path, class: 'navbar-text-color' %></li>        
<li class="navbar-center-item"><%= link_to image_tag('facebook.jpg', class: 'menu-icon'), "http://www.facebook.com/roguetattoos", class: 'navbar-text-color' %></li>
<li class='navbar-center-item'><%= link_to image_tag('instagram.jpg', class: 'menu-icon'), "http://www.instagram.com/ludlowtattoo", class: 'navbar-text-color' %></li>